org.overlord.bam.active.collection.ActiveList list=
	(org.overlord.bam.active.collection.ActiveList)acs.getActiveCollection();

if (list.getSize() == 0) {
	acs.insert(null, value);

} else if (list.getSize() == 1) {
	Object result=list.query(
			new org.overlord.bam.active.collection.QuerySpec());
			
	org.overlord.bam.active.collection.TestObject to=
		(org.overlord.bam.active.collection.TestObject)result.get(0);
		
	to.setName("UPDATED");
	
	acs.update(0, value);
	
} else {
	Object result=list.query(
			new org.overlord.bam.active.collection.QuerySpec());
			
	org.overlord.bam.active.collection.TestObject to=
		(org.overlord.bam.active.collection.TestObject)result.get(0);
		
	acs.remove(0, to);
}
